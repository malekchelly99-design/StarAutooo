{
  "builds": [
    {
      "src": "starauto/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "starauto/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SECRET_KEY": "@django-secret-key",
    "DEBUG": "False",
    "PYTHONPATH": "$PYTHONPATH:starauto"
  }
}
