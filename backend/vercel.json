{
  "builds": [
    {
      "src": "starauto/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "installCommand": "pip install -r requirements.txt",
        "buildCommand": "python manage.py migrate --run-syncdb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "starauto/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SECRET_KEY": "django-star-auto-secret-key-2024-very-secure",
    "DEBUG": "False",
    "PYTHONPATH": "."
  }
}
